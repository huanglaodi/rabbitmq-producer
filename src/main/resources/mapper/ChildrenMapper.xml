<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.rabbitmqproducer.mapper.ChildrenMapper">
    <cache-ref namespace="com.example.rabbitmqproducer.mapper.ChildrenMapper"/>
    <resultMap id="BaseResultMap" type="com.example.rabbitmqproducer.model.Children">
        <id column="id" jdbcType="VARCHAR" property="id" />
        <result column="name" jdbcType="VARCHAR" property="name" />
        <result column="score" jdbcType="INTEGER" property="score" />
    </resultMap>


    <select id='selects' resultMap="BaseResultMap" parameterType="string">
        select * from children_s where id= $id;
    </select>

    <!--获取所有id-->
    <select id="getAllIds" resultType="string">
        select id from children_s;
    </select>
</mapper>